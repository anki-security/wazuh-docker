{
  "description": "Cisco ASA Syslog Parsing Pipeline - Basic template",
  "processors": [
    {
      "grok": {
        "field": "message",
        "patterns": [
          "^<%{POSINT:log.syslog.priority}>%{SYSLOGTIMESTAMP:syslog.timestamp} %{DATA:observer.hostname} : %%{DATA:event.module}-%{INT:event.severity}-%{DATA:event.code}: %{GREEDYDATA:message.text}",
          "^%{GREEDYDATA:message.unparsed}$"
        ],
        "ignore_failure": true
      }
    },
    {
      "set": {
        "field": "observer.vendor",
        "value": "Cisco"
      }
    },
    {
      "set": {
        "field": "observer.product",
        "value": "ASA"
      }
    },
    {
      "set": {
        "field": "observer.type",
        "value": "firewall"
      }
    },
    {
      "set": {
        "field": "ecs.version",
        "value": "8.12.0"
      }
    },
    {
      "append": {
        "field": "event.category",
        "value": ["network"],
        "allow_duplicates": false
      }
    }
  ],
  "on_failure": [
    {
      "set": {
        "field": "error.message",
        "value": "{{ _ingest.on_failure_message }}"
      }
    },
    {
      "set": {
        "field": "event.kind",
        "value": "pipeline_error"
      }
    }
  ]
}
